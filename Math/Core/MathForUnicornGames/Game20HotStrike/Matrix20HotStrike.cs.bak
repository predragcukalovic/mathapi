		/// <summary>
		/// Vraća niz koeficijenata za id simbola.
		/// </summary>
		/// <param name="id"></param>
		/// <returns></returns>
		public static int[] GetSymbolCoefficients(int id)
		{
			var coefficients = new int[5];
			for (var i = 0; i < 5; i++)
			{
				if (id == 0)
				{
					coefficients[i] = WinForScatter20HotStrike[i];
				}
				else if (id == 1)
				{
					coefficients[i] = WinForWild20HotStrike[i];
				}
				else
				{
					coefficients[i] = WinForLines20HotStrike[id, i];
				}
			}
			return coefficients;
		}

