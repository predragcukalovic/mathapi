		public static int[,] GetMatixArray()
		{
			var reelsSet = -1;
			var sum = 0.0;
			var rnd = SoftwareRng.Next();
			var n = _ReelProbs.Length;
			for (var i = 0; i < n; i++)
			{
				sum += _ReelProbs[i];
				if (rnd < sum)
				{
					reelsSet = i;
					break;
				}
			}
			var tmpReelSet = Math.Min(reelsSet, 4);
			var mat = new int[5, 5];
			var matSpec = new int[5, 5];
			var k = SoftwareRng.Next(_Reels[tmpReelSet].Length);
			for (var i = 0; i < 5; i++)
			{
				var l = _Reels[tmpReelSet][k][i].Length;
				var p = SoftwareRng.Next(l);
				var lspec = _ReelSpecial[reelsSet][i].Length;
				var pspec = SoftwareRng.Next(lspec);
				for (var j = 0; j < 5; j++)
				{
					mat[i, j] = _Reels[tmpReelSet][k][i][(p + j) % l];
					matSpec[i, j] = _ReelSpecial[reelsSet][i][(pspec + j) % lspec];
				}
			}
			for (var i = 0; i < 5; i++)
			{
				for (var j = 0; j < 5; j++)
				{
					mat[i, j] = Math.Min(mat[i, j], matSpec[i, j]);
				}
			}
			return mat;
		}

		/// <summary>
		/// Računa dobitak linije.
		/// </summary>
		/// <param name="lineNumber">Broj linije.</param>
		/// <returns></returns>
		public override int CalculateWinLine(int lineNumber)
		{
			var l = GetLine(lineNumber, UnicornGlobalData.GameLineShifted);
			for (var i = 0; i < 5; i++)
			{
				if (l.GetElement(i) == 0)
				{
					l.SetElement(i, 1);
				}
			}
			return l.CalculateLineWin(WinForLines20HotStrike, WinForWild20HotStrike, 1, 1);
		}

		/// <summary>
		/// Daje niz pozicija za simbol.
		/// </summary>
		/// <param name="symbol"></param>
		/// <returns></returns>
		public byte[] GetScatterPositionsArray(int symbol)
		{
			var positions = new byte[5];
			var index = 0;
			for (var i = 0; i < 5; i++)
			{
				for (var j = 1; j < 4; j++)
				{
					if (GetElement(i, j) == symbol)
					{
						positions[index++] = (byte)(j * 5 + i);
					}
				}
			}
			for (; index < 5; index++)
			{
				positions[index] = 255;
			}
			return positions;
		}

		public int GetWinningElement(int line, int win)
		{
			var l = GetLine(line, UnicornGlobalData.GameLineShifted);
			for (var i = 0; i < 5; i++)
			{
				if (l.GetElement(i) == 0)
				{
					l.SetElement(i, 1);
				}
			}
			return l.GetWinningElement(1, win, WinForWild20HotStrike);
		}

		/// <summary>
		/// Daje pozicije dobitnih elemenata.
		/// </summary>
		/// <param name="lineNumber">Broj linije.</param>
		/// <param name="element">Simbol</param>
		/// <returns></returns>
		public byte[] GetLinesPositions(int lineNumber, int element)
		{
			var positionsArray = new byte[5];
			var l = GetLine(lineNumber, UnicornGlobalData.GameLineShifted);
			var i = 0;
			while (i < 5 && (l.GetElement(i) <= 1 || l.GetElement(i) == element))
			{
				positionsArray[i] = (byte)(UnicornGlobalData.GameLineShifted[lineNumber - 1, i] * 5 + i);
				i++;
			}
			for (; i < 5; i++)
			{
				positionsArray[i] = 255;
			}
			return positionsArray;
		}

		/// <summary>
		/// Daje dobitak za sketere.
		/// </summary>
		/// <returns></returns>
		public new int GetScatterWin()
		{
			var count = 0;
			for (var i = 0; i < 5; i++)
			{
				for (var j = 1; j < 4; j++)
				{
					if (GetElement(i, j) == 0)
					{
						count++;
					}
				}
			}
			return count == 0 ? 0 : WinForScatter20HotStrike[count - 1];
		}

		/// <summary>
		/// Konstruiše matricu na osnovu dvodimenzionalnog niza za igru 20HotStrike.
		/// </summary>
		/// <param name="matrix"></param>
		public void FromMatrixArray20HotStrikes(int[,] matrix)
		{
			for (var i = 0; i < 5; i++)
			{
				for (var j = 0; j < 5; j++)
				{
					SetElement(i, j, matrix[i, j]);
				}
			}
		}

